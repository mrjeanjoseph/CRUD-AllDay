@model SportsStore.Web.Models.ProductsListViewModel

@{
    ViewBag.Title = "Product Listings";
}


@foreach (var product in Model.Products) {
    
    @Html.Partial("_ProductSummary", product)
    <hr />
 }

<div class="btn btn-group float-end">
    @Html.PageLinks(Model.PagingInfo, p => Url.Action("ProductListing", new { page = p }))
</div>