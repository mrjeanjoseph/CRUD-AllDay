@model long?
@{ Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Result</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <style>
        body {
            padding-top: 10px;
        }
    </style>

</head>
<body>
    <div class="container w-50">
        <div class="alert alert-info">
            @if (Model.HasValue)
            {
                @: Cache Data: @Model bytes
            }
            else
            {
                @: No Cached Data
            }
        </div>
        @using (Html.BeginForm("PopulateCache", "ChachingTechnique"))
        {
            <button class="btn btn-primary">Populate Cache</button>
        }
    </div>
</body>
</html>